<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:App/etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="madfu-checkout-staging" type="host">checkout-staging.madfu.com.sa</value>
                <value id="madfu-checkout" type="host">checkout.madfu.com.sa</value>
                <value id="google-ajax-cdn" type="host">ajax.googleapis.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="madfu-checkout-staging-css" type="host">checkout-staging.madfu.com.sa</value>
                <value id="madfu-checkout-css" type="host">checkout.madfu.com.sa</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="madfu-checkout-staging-img" type="host">checkout-staging.madfu.com.sa</value>
                <value id="madfu-checkout-img" type="host">checkout.madfu.com.sa</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
